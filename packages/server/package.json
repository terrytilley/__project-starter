{
  "name": "@project-starter/server",
  "version": "1.0.0",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc -b",
    "start": "yarn build && node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./src/config/ormconfig.ts",
    "type-check": "tsc",
    "prepare": "yarn type-check && yarn build"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.12",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.11.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "graphql": "^14.5.8",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.3.1",
    "pg": "^7.14.0",
    "rate-limit-redis": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5",
    "typeorm": "0.2.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-rate-limit": "^3.3.3",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.45",
    "@types/ioredis": "^4.0.20",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.14",
    "@types/nodemailer": "^6.2.2",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-node": "8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.7.2"
  },
  "license": "MIT"
}
